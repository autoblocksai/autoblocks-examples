{
  "name": "prompt-sdk-local",
  "description": "Automated prompt versioning and typesafe prompt building with the local prompt SDK",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "npm run gen && dotenv -e .env -- tsx ./src/index.ts",
    "gen": "prompts generate",
    "gen:watch": "nodemon --quiet --watch prompt-templates --ext \"*\" --exec \"npm run gen\""
  },
  "license": "MIT",
  "dependencies": {
    "@autoblocks/client": "^0.0.34",
    "@types/node": "^20.9.1",
    "dotenv-cli": "^7.3.0",
    "nodemon": "^3.0.1",
    "openai": "^4.6.0",
    "tsx": "^4.1.3",
    "typescript": "^5.2.2"
  },
  "autoblocks": {
    "promptsDirectory": "prompt-templates"
  }
}
